
@{
    ViewBag.Title = "test";
}
<div class="row">
    <input type="date" id="start" name="trip-start"
           value="2018-07-22"
           min="2018-01-01" max="@DateTime.Now.Year.ToString()-@DateTime.Now.Month.ToString()-@DateTime.Now.Day.ToString()">
</div>
<h2>test</h2>
<table id="table" border="1">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
    </tr>
    <tr>
        <td>A1</td>
        <td>B1</td>
        <td>15</td>
    </tr>
    <tr>
        <td>A2</td>
        <td>B2</td>
        <td>15</td>
    </tr>
    <tr>
        <td>A3</td>
        <td>B3</td>
        <td>15</td>
    </tr>
</table>
<span id="resultado"></span>

@using (Html.BeginForm("testForm", "RegistroMovimiento", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="form-actions no-color">
        <input type="text" id="lista" name="lista[]" value="0" style="display:none" />
        <p id="lista2">
        </p>
        <input type="submit" value="Test" class="btn boton-estilo" onclick="action()" />
    </div>
}
<script>

    var table = document.getElementById("table");
    const valores = [];
    for (var i = 1; i < table.rows.length; i++) {
        valores[i] = table.rows[i].cells[0].innerHTML
    }
    document.getElementById("lista").value = valores;
</script>
